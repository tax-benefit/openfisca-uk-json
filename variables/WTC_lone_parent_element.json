      "file_path": "/openfisca_uk/variables/finance/benefit/family/tax_credits.py",
      "parameters": [
        "benefit.tax_credits.working_tax_credit.elements.lone_parent"
      ],
      "source_code": "def formula(benunit, period, parameters):\n    WTC = parameters(period).benefit.tax_credits.working_tax_credit\n    family_type = benunit(\"family_type\", period)\n    families = family_type.possible_values\n    amount = (\n        family_type == families.LONE_PARENT\n    ) * WTC.elements.lone_parent\n    return (\n        benunit(\"is_WTC_eligible\", period)\n        * benunit(\"claims_WTC\", period)\n        * amount\n    )\n",
      "start_line_number": 319,
      "stop_line_number": 331,
      "variables": [
        "claims_WTC",
        "family_type",
        "is_WTC_eligible"
      ]
    }
