      "file_path": "/openfisca_uk/variables/finance/tax/income_tax/allowances.py",
      "parameters": [
        "tax.income_tax.allowances.personal_savings_allowance.additional",
        "tax.income_tax.allowances.personal_savings_allowance.basic",
        "tax.income_tax.allowances.personal_savings_allowance.higher"
      ],
      "source_code": "def formula(person, period, parameters):\n    tax_band = person(\"tax_band\", period)\n    tax_bands = tax_band.possible_values\n    amounts = parameters(\n        period\n    ).tax.income_tax.allowances.personal_savings_allowance\n    allowance = select(\n        [\n            tax_band == tax_bands.ADDITIONAL,\n            tax_band == tax_bands.HIGHER,\n            tax_band == tax_bands.BASIC,\n            tax_band == tax_bands.NONE,\n        ],\n        [amounts.additional, amounts.higher, amounts.basic, amounts.basic],\n    )\n    return allowance\n",
      "start_line_number": 134,
      "stop_line_number": 150,
