        "name": "formula"
      },
      "file_path": "/openfisca_uk/variables/finance/benefit/family/tax_credits.py",
      "parameters": [
        "benefit.tax_credits.working_tax_credit.elements.childcare_1",
        "benefit.tax_credits.working_tax_credit.elements.childcare_2",
        "benefit.tax_credits.working_tax_credit.elements.childcare_coverage"
      ],
      "source_code": "def formula(benunit, period, parameters):\n    WTC = parameters(period).benefit.tax_credits.working_tax_credit\n    num_children = benunit(\"num_children\", period)\n    max_childcare_amount = (\n        num_children == 1\n    ) * WTC.elements.childcare_1 * WEEKS_IN_YEAR + (\n        num_children > 1\n    ) * WTC.elements.childcare_2 * WEEKS_IN_YEAR\n    childcare_element = min_(\n        max_childcare_amount,\n        WTC.elements.childcare_coverage\n        * benunit.sum(benunit.members(\"childcare_cost\", period)),\n    )\n    return (\n        benunit(\"is_WTC_eligible\", period)\n        * benunit(\"claims_WTC\", period)\n        * childcare_element\n    )\n",
      "start_line_number": 403,
      "stop_line_number": 421,
      "variables": [
        "childcare_cost",
        "claims_WTC",
        "is_WTC_eligible",
        "num_children"
      ]
    }
