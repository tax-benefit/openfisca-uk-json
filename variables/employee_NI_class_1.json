      "file_path": "/openfisca_uk/variables/finance/tax/national_insurance/class_1.py",
      "parameters": [
        "tax.national_insurance.class_1.rates.employee.additional",
        "tax.national_insurance.class_1.rates.employee.main",
        "tax.national_insurance.class_1.thresholds.primary_threshold",
        "tax.national_insurance.class_1.thresholds.upper_earnings_limit"
      ],
      "source_code": "def formula(person, period, parameters):\n    class_1 = parameters(period).tax.national_insurance.class_1\n    earnings = person(\"employment_income\", period)\n    main_earnings = amount_between(\n        earnings,\n        class_1.thresholds.primary_threshold * WEEKS_IN_YEAR,\n        class_1.thresholds.upper_earnings_limit * WEEKS_IN_YEAR,\n    )\n    add_earnings = amount_over(\n        earnings, class_1.thresholds.upper_earnings_limit * WEEKS_IN_YEAR\n    )\n    charge = (\n        class_1.rates.employee.main * main_earnings\n        + add_earnings * class_1.rates.employee.additional\n    )\n    return charge\n",
      "start_line_number": 24,
      "stop_line_number": 40,
      "variables": [
        "employment_income"
      ]
    }
