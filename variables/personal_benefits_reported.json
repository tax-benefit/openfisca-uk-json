      "source_code": "def formula(person, period, parameters):\n    BENEFITS = [\n        \"AA\",\n        \"AFCS\",\n        \"BSP\",\n        \"carers_allowance\",\n        \"DLA_M\",\n        \"DLA_SC\",\n        \"ESA_contrib\",\n        \"IIDB\",\n        \"incapacity_benefit\",\n        \"JSA_contrib\",\n        \"PIP_M\",\n        \"PIP_DL\",\n        \"SDA\",\n        \"state_pension\",\n    ]\n    return add(\n        person, period, map(lambda ben: ben + \"_reported\", BENEFITS)\n    )\n",
