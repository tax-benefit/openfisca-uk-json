        "name": "formula"
      },
      "file_path": "/openfisca_uk/variables/finance/benefit/family/universal_credit.py",
      "parameters": [
        "benefit.universal_credit.allowances.basic_couple_older",
        "benefit.universal_credit.allowances.basic_couple_young",
        "benefit.universal_credit.allowances.basic_single_older",
        "benefit.universal_credit.allowances.basic_single_young"
      ],
      "source_code": "def formula(benunit, period, parameters):\n    UC = parameters(period).benefit.universal_credit\n    is_single = benunit(\"is_single\", period)\n    is_couple = benunit(\"is_couple\", period)\n    is_over_25 = benunit(\"youngest_adult_age\", period.this_year) >= 25\n    basic_amount = (\n        is_single * not_(is_over_25) * UC.allowances.basic_single_young\n        + is_single * is_over_25 * UC.allowances.basic_single_older\n        + is_couple * not_(is_over_25) * UC.allowances.basic_couple_young\n        + is_couple * is_over_25 * UC.allowances.basic_couple_older\n    ) * MONTHS_IN_YEAR\n    return basic_amount\n",
      "start_line_number": 36,
      "stop_line_number": 48,
      "variables": [
        "is_couple",
        "is_single",
        "youngest_adult_age"
      ]
    }
