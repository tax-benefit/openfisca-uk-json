      "file_path": "/openfisca_uk/variables/finance/benefit/family/tax_credits.py",
      "parameters": [
        "benefit.tax_credits.working_tax_credit.elements.disabled",
        "benefit.tax_credits.working_tax_credit.min_hours.lower"
      ],
      "source_code": "def formula(benunit, period, parameters):\n    WTC = parameters(period).benefit.tax_credits.working_tax_credit\n    condition = (\n        benunit.sum(\n            (benunit.members(\"is_disabled_for_benefits\", period) > 0)\n            * benunit.members(\"is_adult\", period)\n            * benunit.members(\"weekly_hours\", period)\n        )\n        > WTC.min_hours.lower\n    ) > 0\n    amount = condition * WTC.elements.disabled\n    return (\n        benunit(\"is_WTC_eligible\", period)\n        * benunit(\"claims_WTC\", period)\n        * amount\n    )\n",
      "start_line_number": 340,
      "stop_line_number": 356,
      "variables": [
        "claims_WTC",
        "is_WTC_eligible",
        "is_adult",
        "is_disabled_for_benefits",
