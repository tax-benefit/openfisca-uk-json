      "parameters": [
        "benefit.universal_credit.elements.carer_element",
        "benefit.universal_credit.elements.child_element",
        "benefit.universal_credit.elements.childcare_cost_rate",
        "benefit.universal_credit.elements.disabled_child_element",
        "benefit.universal_credit.elements.disabled_element",
